@model WebApplication1.Models.Localization.Home.ContactViewModel

@{
    ViewBag.Title = @Model.Title;
}

<main aria-labelledby="title">
    <h2 id="title"> @Model.Title</h2>
    <h3> @Model.Message</h3>

    <address>
        One Microsoft Way<br />
        Redmond, WA 98052-6399<br />
        <abbr title="Phone">P:</abbr>
        425.555.0100
    </address>

    <address>
        <strong> @Model.Support</strong> <a href="mailto:Support@example.com">Support@example.com</a><br />
        <strong> @Model.Marketing</strong> <a href="mailto:Marketing@example.com">Marketing@example.com</a>
    </address>
</main>